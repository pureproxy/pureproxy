{
  "name": "@pureproxy/pureproxy",
  "version": "1.1.1",
  "description": "Pure, intercepting proxy server designed with simplicity and extensibility in mind",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pureproxy/pureproxy.git"
  },
  "bugs": {
    "url": "https://github.com/pureproxy/pureproxy/issues"
  },
  "homepage": "https://github.com/pureproxy/pureproxy#readme",
  "author": "pdparchitect",
  "keywords": [
    "proxy",
    "server",
    "stream",
    "http",
    "https",
    "websocket",
    "websockets"
  ],
  "type": "module",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:types": "npx -p typescript tsc --declaration --emitDeclarationOnly --outDir types",
    "format": "run-s format:*",
    "format:pkg": "npx format-package -w",
    "format:src": "npx prettier -w examples/ lib/",
    "install": "npx husky install",
    "lint": "eslint lib/",
    "test": "true"
  },
  "types": "types/index.d.ts",
  "dependencies": {
    "http-parser-js": "^0.5.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.15",
    "eslint": "8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-require-path-exists": "^1.1.9",
    "npm-run-all": "^4.1.5"
  }
}
